{
  "name": "test",
  "version": "1.0.0",
  "description": "",
  "main": "src/server.ts",
  "scripts": {
    "start": "db-migrate up && nodemon src/server.ts",
    "build": "npx tsc",
    "build2": "tsc --build",
    "watch": "db-migrate up && tsc-watch --esModuleInterop src/server.ts --outDir ./dist --onSuccess \"node ./dist/server.js\"",
    "migrate": "db-migrate --env test up && db-migrate up",
    "jasmine": "jasmine",
    "test": "set ENV=test&& db-migrate --env test up && npm run build && npm run jasmine && db-migrate db:drop test",
    "tests": "set ENV=test&& db-migrate --env test up && npm run build && mocha -r ts-node/register 'src/handlers/__tests__/**/*.ts' && db-migrate db:drop test",
    "tests0": "set ENV=test&&npm run build && jest",
    "typings": "typings install",
    "lint": "eslint src/**/*.ts --ext .js,.ts",
    "lint-fix": "eslint --fix --ext src/**/*.ts",
    "lintfix": "eslint src/**/*.ts --quiet --fix",
    "prettier": "prettier --config .prettierrc src/**/*.ts --write",
    "tsc": "tsc"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "jest": {
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.json"
      }
    },
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "**/__tests__/*.+(ts|tsx|js)"
    ],
    "testEnvironment": "node"
  },
  "dependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/jquery": "^3.5.14",
    "@types/jsonwebtoken": "^8.5.8",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.20.0",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.7.3",
    "pg-types": "^4.0.0",
    "postgres": "^3.2.4"
  },
  "devDependencies": {
    "@types/chai": "^4.3.1",
    "@types/chai-http": "^4.2.0",
    "@types/jasmine": "^4.0.3",
    "@types/jest": "^28.1.3",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.0.0",
    "@types/pg": "^8.6.5",
    "@types/request": "^2.48.8",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "chai": "^4.3.6",
    "chai-http": "^4.3.0",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "jasmine": "^4.2.0",
    "jasmine-spec-reporter": "^7.0.0",
    "jasmine-ts": "^0.4.0",
    "jest": "^28.1.1",
    "mocha": "^10.0.0",
    "nodemon": "^2.0.16",
    "prettier": "^2.7.1",
    "request": "^2.88.2",
    "supertest": "^6.2.3",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.8.1",
    "tsc-watch": "^5.0.3",
    "typescript": "^4.7.4"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}
